package Practice;
import java.util.Scanner;

public class forloop {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("please enter your number");
		int n=sc.nextInt();
		
		int table;
		
		for (int i=1; i<=10; i++)
		{
			table=n*i;
			System.out.println("your table is  "  +table);
		}
		
	}

}
