package Practice;
import java.util.Scanner;

public class Loop {

	public static void main(String[] args) {
		int i;
		int sum;
		System.out.println("please enter your value");
		
		Scanner sc=new Scanner (System.in);
		sum=sc.nextInt();
		
		
	
		for(i=1; i<=5; i++)
		{
			sum= 2*i ;
			System.out.println(sum);
		}

	}

}
